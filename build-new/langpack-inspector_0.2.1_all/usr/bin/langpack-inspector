#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

try:
    from langpack_inspector.main import main
    main()
except ImportError:
    try:
        from langpack_inspector.app import main
        main()
    except ImportError:
        print("Error: Cannot find main module for langpack-inspector", file=sys.stderr)
        sys.exit(1)
